  "sandbox_attack_tests": [
    {
      "id": "sa1",
      "path": "../../../etc/passwd",
      "description": "路径遍历攻击 - 访问系统文件",
      "should_block": true
    },
    {
      "id": "sa2",
      "path": "/etc/shadow",
      "description": "绝对路径攻击 - 访问密码文件",
      "should_block": true
    },
    {
      "id": "sa3",
      "path": "workspace/../../../root/.ssh/id_rsa",
      "description": "混合路径遍历攻击",
      "should_block": true
    },
    {
      "id": "sa4",
      "path": "workspace/documents/../../etc/hosts",
      "description": "深层路径遍历攻击",
      "should_block": true
    },
    {
      "id": "sa5",
      "path": "workspace/test.txt",
      "description": "正常路径 - 应该允许",
      "should_block": false
    },
    {
      "id": "sa6",
      "path": "workspace/data/config.json",
      "description": "正常子目录路径 - 应该允许",
      "should_block": false
    },
    {
      "id": "sa7",
      "path": ".env",
      "description": "敏感文件名攻击",
      "should_block": true
    },
    {
      "id": "sa8",
      "path": "workspace/private_key.txt",
      "description": "敏感文件名模式攻击",
      "should_block": true
    },
    {
      "id": "sa9",
      "path": "workspace/.ssh/config",
      "description": "SSH配置文件攻击",
      "should_block": true
    },
    {
      "id": "sa10",
      "path": "workspace/logs/app.log",
